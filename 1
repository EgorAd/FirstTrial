using System;
using System.Collections.Generic;
using System.Linq;
using System.Text.RegularExpressions;

namespace HelloWorld
{
	public class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("Введите выражение 1: Точное");
   			double a1 = Convert.ToDouble(Console.ReadLine());
			Console.WriteLine("Введите выражение 1: Приблизительное ");
			double a2 = Convert.ToDouble(Console.ReadLine());
			Console.WriteLine("Число равно " + a2);
			Console.WriteLine("Введите выражение 2: Точное ");
   			double b1 = Convert.ToDouble(Console.ReadLine());
			Console.WriteLine("Введите выражение 2: Приблизительное ");
			double b2 = Convert.ToDouble(Console.ReadLine());
			Console.WriteLine("Число равно" + b2);
			double A = Math.Abs(a1 - a2);
			Console.WriteLine("Выражение 1: Абс.Погрешность = " + "{0: 0.######}",A);
			double B = Math.Abs(b1 - b2);
			Console.WriteLine("Выражение 2: Абс.Погрешность = " + "{0: 0.######}",B);
  			Console.WriteLine("Введите выражение 1: Абс.Погрешность");
			double Apogr = Convert.ToDouble(Console.ReadLine());
   			double AP = Apogr / a2;
      			Console.WriteLine("Выражение 1: Отн.Погрешность = " + "{0: 0.######}",AP);
			Console.WriteLine("Введите выражение 2: Абс.Погрешность");
			double Bpogr = Convert.ToDouble(Console.ReadLine());
   			double BP = Bpogr / b2;
			Console.WriteLine("Выражение 1: Отн.Погрешность = " + "{0: 0.######}",BP);
   			if (AP > BP) Console.WriteLine("Выражение 2 точнее")
      			else Console.WriteLine("Выражение 1 точнее")
			Console.ReadKey();
		}
	}
}
